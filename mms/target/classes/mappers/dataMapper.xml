<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="global.sesoc.mms.dao.dataMapper">
	
	<select id="selectIntakeInfoGet" resultType="global.sesoc.mms.dto.data">
		select 
			* 
		from 
			data 
		where 
			userid = #{userid} 
		and 
			regdate = #{regdate} 
	</select>

</mapper>